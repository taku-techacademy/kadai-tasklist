<% if logged_in? %>
<h1>タスク一覧</h1>

<ul>
    <% @tasks.each do |task| %>
        <li><%= link_to task.id, task %> : <%= task.content %> &gt; <%= task.status %></li>
    <% end %>
</ul>

<%= link_to '新規タスクの投稿', new_task_path %>
<%= link_to 'Log_out', logout_path %>

<% else %>
<%= link_to 'Log_in', login_path %>
<% end %>